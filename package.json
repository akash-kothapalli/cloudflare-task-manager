{
	"name": "cloudflare-task-manager",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"dev": "wrangler dev --local",
		"deploy": "wrangler deploy",
		"test": "vitest",
		"type-check": "tsc --noEmit",
		"db:migrate:local": "wrangler d1 execute task-manager-db --local --file=src/db/schema.sql",
		"db:migrate:remote": "wrangler d1 execute task-manager-db --remote --file=src/db/schema.sql",
		"cf-typegen": "wrangler types --env-interface CloudflareBindings"
	},
	"devDependencies": {
		"@cloudflare/vitest-pool-workers": "^0.5.41",
		"@cloudflare/workers-types": "^4.20250214.0",
		"typescript": "^5.5.2",
		"vitest": "^2.1.8",
		"wrangler": "^3.99.0"
	},
	"dependencies": {
		"jose": "^6.1.3"
	}
}
